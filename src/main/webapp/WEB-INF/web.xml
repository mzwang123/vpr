<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
<display-name>VehiclePreRegistratin</display-name>
    <context-param>
        <param-name>webAppRootKey</param-name>
        <param-value>vehpreregistration.root</param-value>
    </context-param>
    <context-param>
        <param-name>logbackConfigLocation</param-name>
        <param-value>src/main/webapp/WEB-INF/logback.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>        
    </listener>
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>src/main/webapp/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <servlet>
        <servlet-name>do</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>
</web-app>
